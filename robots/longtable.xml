<mujoco model="longtable">
    <compiler meshdir="assets" texturedir="assets"/>
    <asset>
        <texture name="texmarble" type="cube" file="marble.png"/>
        <material name="matmarble" texture="texmarble" rgba=".7 .7 .7 1"/>
        <mesh name="longtable_mesh" file="longtable.stl" scale="0.35 0.7 1.1" />
        <!--base size is 0.8m-->
    </asset>

    <default>
        <default class="collision">
            <geom condim="6" group="0" />
        </default>
        <default class="visual">
            <geom condim="6" group="1" conaffinity="0" contype="0" />
        </default>
    </default>

    <worldbody>
      <body name="base_link">
		<light mode="fixed" directional="false" diffuse=".8 .8 .8" specular="0.3 0.3 0.3" pos="0 0 -4.0" dir="0 0 1" active="false"/>
        <geom name ="main" class="collision" type="box" size="0.175 0.525 .022" rgba="0.266754 0.174546 0.0644028 1" />
        <geom class="collision" type="cylinder" size="0.021 0.2" pos="0.14 -0.49 0.66"  rgba="0.266754 0.174546 0.0644028 1" />
        <geom class="collision" type="cylinder" size="0.021 0.2" pos="-0.14 -0.49 0.66"  rgba="0.266754 0.174546 0.0644028 1" />
        <geom class="collision" type="cylinder" size="0.021 0.2" pos="0.14 0.49 0.66"  rgba="0.266754 0.174546 0.0644028 1" />
        <geom class="collision" type="cylinder" size="0.021 0.2" pos="-0.14 0.49 0.66"  rgba="0.266754 0.174546 0.0644028 1" />
        <geom class="visual" type="mesh" mesh="longtable_mesh" material="matmarble" />
      </body>
    </worldbody>
</mujoco>
