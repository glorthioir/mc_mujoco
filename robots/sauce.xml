<mujoco model="sauce">
	<compiler meshdir="assets" texturedir="assets" />
    <asset>
        <mesh name="sauce_mesh" file="sauce.obj" scale="0.011 0.011 0.011" />
	<texture name="texgeom" type="cube" builtin="gradient" width="128" height="128"
		 rgb1="1 0.1 0.3" rgb2="1 0.7 0.7"/>
	<material name='sauceObject' texture="texgeom" texuniform="true"/>
    </asset>

    <default>
        <default class="collision">
            <geom condim="6" group="0" />
        </default>
        <default class="visual">
        	<geom condim="6" conaffinity="0" contype="0" mass="0" gap="10"/>
        </default>
    </default>

    <worldbody>
      <light mode="targetbodycom" target="base_link" directional="false" diffuse=".8 .8 .8" specular="0.3 0.3 0.3" pos="0 0 4.0" dir="0 0 -1" active="false"/>
      <body name="base_link">
        <freejoint/>
        <inertial pos="0 0 0.03" mass="0.2" diaginertia="0.0003 0.0003 0.000047"/>
        <geom class="collision" name ="collision" type="cylinder" friction="1 0.03 0.01" size="0.022 0.065" pos="0.0 0.0 0.065"/>
        <geom class="visual" group="1" type="mesh" mesh="sauce_mesh" material="sauceObject" />
        <geom name ="objectPos" class="visual" group="4" type="sphere" size="0.005"  pos="0.0 0.0 0.07" euler="0 0 0"/>
        <!--<geom name ="grabbingPos1" class="visual" group="4" type="cylinder" size="0.002 0.005"  pos="0.0 0.0 0.15" euler="0 0 0"/>-->
        <geom name ="grabbingPos1" class="visual" group="4" type="cylinder" size="0.005 0.015"  pos="0.03 0 0.07"/>
        <geom name ="grabbingPos2" class="visual" group="4" type="cylinder" size="0.005 0.015"  pos="-0.03 0 0.07"/>
        <geom name ="grabbingPos3" class="visual" group="4" type="cylinder" size="0.005 0.015"  pos="0 0.03 0.07"/>
        <geom name ="grabbingPos4" class="visual" group="4" type="cylinder" size="0.005 0.015"  pos="0 -0.03 0.07"/>
      </body>
    </worldbody>
</mujoco>
